{
    "name": "superrosko/example-php-ffi-windows-hello-world-msgbox",
    "description": "A simple example of using PHP FFI for Windows api.",
    "homepage": "https://github.com/superrosko/example-php-ffi-windows-hello-world-msgbox",
    "license": "MIT",
    "version": "0.0.1",
    "authors": [
        {
            "name": "Dmitriy Bespalov",
            "email": "dbespalov.rosko@gmail.com",
            "homepage": "https://rdevelab.ru",
            "role": "Developer"
        }
    ],
    "keywords": [
        "php",
        "ffi",
        "Windows"
    ],
    "autoload": {
        "psr-4": {
            "Superrosko\\ExamplePhpFFI\\": "src/"
        }
    },
    "require": {
        "php": ">=8.0",
        "ext-ffi": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.17",
        "psalm/plugin-phpunit": "^0.15",
        "vimeo/psalm": "^4.3.1"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "scripts": {
        "check-style": "set PHP_CS_FIXER_IGNORE_ENV=1 && php-cs-fixer fix --dry-run --diff",
        "fix-style": "set PHP_CS_FIXER_IGNORE_ENV=1 && php-cs-fixer fix",
        "static-analysis": "psalm --shepherd --stats",
        "static-analysis-log": "psalm --no-cache --show-info=true"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
